{<%# LinkedIn oauth token mapping %>
  "id": "authz_req|Linkedin|<%= @oauth_svc_acc_tok_mgr_id -%>",
  "context": {
    "type": "IDP_ADAPTER", 
    "contextRef": {
      "id": "Linkedin", 
      "location": "<%= @adm_api_baseURL -%>/idp/adapters/Linkedin"
    }
  }, 
  "accessTokenManagerRef": {
    "id": "<%= @oauth_svc_acc_tok_mgr_id -%>", 
    "location": "<%= @adm_api_baseURL -%>/oauth/accessTokenManagers/<%= @oauth_svc_acc_tok_mgr_id -%>"
  }, 
  "attributeContractFulfillment": {
<% @linkedin_oauth_token_map.each_with_index do |val, i| -%>
      "<%= val['name'] -%>": {
        "source": {
          "type": "<%= val['type'] -%>"
        },
        "value": "<%= val['value'] -%>"
      }<%= ',' if i < (@linkedin_oauth_token_map.size - 1) %>
<% end -%>
  }
}
