{<%# Google oauth token mapping %>
  "id": "authz_req|Google|<%= @oauth_svc_acc_tok_mgr_id -%>",<%# I hardcoded Google but not the oauth token mgr. %>
  "context": {
    "type": "IDP_ADAPTER", 
    "contextRef": {
      "id": "Google", 
      "location": "<%= @adm_api_baseURL -%>/idp/adapters/Google"
    }
  }, 
  "accessTokenManagerRef": {
    "id": "<%= @oauth_svc_acc_tok_mgr_id -%>", 
    "location": "<%= @adm_api_baseURL -%>/oauth/accessTokenManagers/<%= @oauth_svc_acc_tok_mgr_id -%>"
  }, 
  "attributeContractFulfillment": {
<% @google_oauth_token_map.each_with_index do |val, i| -%>
      "<%= val['name'] -%>": {
        "source": {
          "type": "<%= val['type'] -%>"
        },
        "value": "<%= val['value'] -%>"
      }<%= ',' if i < (@google_oauth_token_map.size - 1) %>
<% end -%>
  }
}
