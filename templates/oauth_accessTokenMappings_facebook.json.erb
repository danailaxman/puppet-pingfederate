{<%# Facebook oauth token mapping %>
  "id": "authz_req|Facebook|<%= @oauth_svc_acc_tok_mgr_id -%>",<%# I hardcoded Facebook but not the oauth token mgr. %>
  "context": {
    "type": "IDP_ADAPTER", 
    "contextRef": {
      "id": "Facebook", 
      "location": "<%= @adm_api_baseURL -%>/idp/adapters/Facebook"
    }
  }, 
  "accessTokenManagerRef": {
    "id": "<%= @oauth_svc_acc_tok_mgr_id -%>", 
    "location": "<%= @adm_api_baseURL -%>/oauth/accessTokenManagers/<%= @oauth_svc_acc_tok_mgr_id -%>"
  }, 
  "attributeContractFulfillment": {
<% @facebook_oauth_token_map.each_with_index do |val, i| -%>
      "<%= val['name'] -%>": {
        "source": {
          "type": "<%= val['type'] -%>"
        },
        "value": "<%= val['value'] -%>"
      }<%= ',' if i < (@facebook_oauth_token_map.size - 1) %>
<% end -%>
  }
}
